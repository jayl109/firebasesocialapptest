# platform :ios, '9.0'
target 'firebase username test' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Firebase'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'

  # Pods 

  target 'firebase username testTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'firebase username testUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
